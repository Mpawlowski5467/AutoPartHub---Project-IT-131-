<% if (allrecs.length > 0) {%>
    <table border=1>
        <tr>
            <th> ID </th>
            <th> First Name </th>
            <th> Last Name </th>
            <th> Address 1 </th>
            <th> Address 2 </th>
            <th> City </th>
            <th> State </th>
            <th> Zip Code </th>
            <th> Country </th>
            <th> Email </th>
            <th> Phone Number </th>
            <th> Username </th>
            <!-- <th> Password </th> -->
        </tr>
    <% allrecs.forEach((recref, index) => { %>
    <tr>
    <td><%= recref.ID %></td>
    <td><%= recref.FirstName %></td>
    <td><%= recref.LastName %></td>
    <td><%= recref.Address1 %></td>
    <td><%= recref.Address2 %></td>
    <td><%= recref.City %></td>
    <td><%= recref.State %></td>
    <td><%= recref.Zip %></td>
    <td><%= recref.Country %></td>
    <td><%= recref.Email %></td>
    <td><%= recref.PhoneNumber %></td>
    <td><%= recref.Username %></td>
    <!-- <td><%= recref.Password %></td> -->
    <td><a href='/customer/<%= recref.ID %>/show'> Details</a></td>
    <td><a href='/customer/<%= recref.ID%>/edit'>Edit</a></td>
    <td><a href='/customer/<%= recref.ID %>/delete' onclick="return confirm('Are you sure you want to delete this record?');">Delete</a></td>
    </tr>
    <% }); %>
    </table>
    <% } else { %>
    <p>Not Records Available</p>
    <% } %>
    <p> <a href='/customer/addrecord'>Add New</a> </p>
    